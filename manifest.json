{
    "manifest_version": 2,
    "name": "Youtube Ad-Skipper",
    "version": "1.0",
  
    "description": "Skips youtube ads",

    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/watch*"],
        "js": ["adskipper.js"],
        "run_at": "document_end"
      }
    ]
  }
  