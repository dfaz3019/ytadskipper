{
    "manifest_version": 2,
    "name": "WebNavigation Example",
    "version": "1.0",
    "permissions": ["webNavigation", "activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["adskipper.js"],
        "css": ["styles.css"]
      }
    ]
  }
  